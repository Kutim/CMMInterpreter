/* Bubble sort
 * Test control flow and its nests
 */

int myArr[3];
myArr[0]=2;
myArr[1]=3;
myArr[2]=0;
int count;
count = 0;
// Output unsorted array
while(count < 3) {
	write(myArr[count]);
	count = count+1;
}
// Bubble sort array
int count2;
count = 0;
while (count < 2) {
	count2 = 0;
	while (count2 < (2 - count)) {
		if (myArr[count2] > myArr[count2+1]) {
			int tmp;
			tmp = myArr[count2];
			myArr[count2] = myArr[count2+1];
			myArr[count2+1] = tmp;
		}
		count2 = count2 + 1;
	}
	count = count + 1;
}
count = 0;
// Output sorted array
while(count < 3) {
	write(myArr[count]);
	count = count+1;
}
